{
	"extends": "google",
	"env":
	{
		"node": true,
		"browser": false,
		"es6": true
	},
	"rules":
	{
		"brace-style": ["error", "allman", {"allowSingleLine": true}],
		"no-tabs": "off",
		"no-console": "warn",
		"comma-dangle": ["error", "never"],
		"new-cap": "off",
		"max-len": ["warn", 120],
		"space-before-function-paren": ["error", {"anonymous": "always", "named": "never"}],
		"camelcase": "warn"
	}
}